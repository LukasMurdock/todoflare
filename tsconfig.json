{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"types": ["vite/client", "@cloudflare/workers-types/2023-07-01"],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"skipLibCheck": true
	}
}
